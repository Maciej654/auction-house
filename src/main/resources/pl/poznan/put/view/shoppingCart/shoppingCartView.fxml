    <Label fx:id="errorLabel" layoutX="182.0" layoutY="34.0" prefHeight="17.0" prefWidth="406.0" textFill="#f50000" />
   <Button fx:id="userPageButton" layoutX="9.0" layoutY="2.0" mnemonicParsing="false" text="user page" />
</AnchorPane>
