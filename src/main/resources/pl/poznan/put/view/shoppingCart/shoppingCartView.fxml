            <TableColumn fx:id="auctionNameColumn" prefWidth="169.0" text="auction name" />
            <TableColumn fx:id="itemNameColumn" prefWidth="125.0" text="item name" />
            <TableColumn fx:id="sellerColumn" prefWidth="151.0" text="seller" />
            <TableColumn fx:id="addressColumn" minWidth="0.0" prefWidth="132.0" text="address" />
            <TableColumn fx:id="acceptColumn" prefWidth="154.0" text="accept" />
