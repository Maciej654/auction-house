<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.DatePicker?><?import javafx.scene.control.Label?><?import javafx.scene.control.PasswordField?><?import javafx.scene.control.TextField?><?import javafx.scene.control.ToolBar?><?import javafx.scene.layout.ColumnConstraints?><?import javafx.scene.layout.GridPane?><?import javafx.scene.layout.Pane?><?import javafx.scene.layout.RowConstraints?><?import javafx.scene.layout.VBox?>
<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.poznan.put.controller.user.info.UserInfo">
    <ToolBar minHeight="-Infinity" minWidth="-Infinity">
        <Button mnemonicParsing="false" onMouseClicked="#backButtonClick" text="&lt;" />
        <Pane fx:id="spacePane" minHeight="-Infinity" minWidth="-Infinity" />
        <Button fx:id="saveButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            mnemonicParsing="false" onMouseClicked="#saveButtonClick" text="Save" />
    </ToolBar>
    <VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
        spacing="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <GridPane alignment="CENTER" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefWidth="600.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="-Infinity" percentWidth="40.0" />
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="-Infinity" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
                    vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
                    vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
                    vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
                    vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
                    vgrow="SOMETIMES" />
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <Label text="Email:" />
            <Label text="First name:" GridPane.rowIndex="1" />
            <Label text="Last name:" GridPane.rowIndex="2" />
            <Label text="Birthday:" GridPane.rowIndex="3" />
            <Label text="Password:" GridPane.rowIndex="4" />
            <TextField fx:id="emailTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" prefWidth="300.0" promptText="john.doe@example.com" GridPane.columnIndex="1" />
            <TextField fx:id="firstNameTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" promptText="John" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="lastNameTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" promptText="Doe" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <DatePicker fx:id="birthdayDatePicker" editable="false" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" promptText="DD/MM/YYYY" GridPane.columnIndex="1"
                GridPane.rowIndex="3" />
            <PasswordField fx:id="passwordField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" promptText="password" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        </GridPane>
    </VBox>
</VBox>
