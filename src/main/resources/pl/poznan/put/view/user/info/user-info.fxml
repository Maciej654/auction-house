<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.DatePicker?><?import javafx.scene.control.Label?><?import javafx.scene.control.PasswordField?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.ColumnConstraints?><?import javafx.scene.layout.GridPane?><?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" hgap="10.0" minHeight="-Infinity" minWidth="-Infinity"
    prefWidth="600.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1"
    fx:controller="pl.poznan.put.controller.user.info.UserInfo">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="-Infinity" percentWidth="40.0" />
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="-Infinity" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" valignment="CENTER"
            vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Email:" />
        <TextField GridPane.columnIndex="1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefWidth="300.0" promptText="john.doe@example.com" fx:id="emailTextField" />
        <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" promptText="John" fx:id="firstNameTextField" />
        <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" promptText="Doe" fx:id="lastNameTextField" />
        <Label GridPane.rowIndex="1" text="First name:" />
        <Label GridPane.rowIndex="2" text="Last name:" />
        <Label GridPane.rowIndex="3" text="Birthday:" />
        <Label GridPane.rowIndex="4" text="Password:" />
        <PasswordField GridPane.columnIndex="1" GridPane.rowIndex="4" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" promptText="password" fx:id="passwordField" />
        <DatePicker GridPane.columnIndex="1" GridPane.rowIndex="3" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" promptText="DD/MM/YYYY" fx:id="birthdayDatePicker" />
        <Button GridPane.columnIndex="1" GridPane.rowIndex="5" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#saveButtonClick"
            prefWidth="75.0" text="Save" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
