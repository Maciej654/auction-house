<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.Label?><?import javafx.scene.control.Tab?><?import javafx.scene.control.TabPane?><?import javafx.scene.layout.BorderPane?><?import javafx.scene.layout.ColumnConstraints?><?import javafx.scene.layout.GridPane?><?import javafx.scene.layout.RowConstraints?><?import javafx.scene.layout.VBox?><?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
    minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1"
    fx:controller="pl.poznan.put.controller.auction.details.AuctionDetailsController">
    <GridPane BorderPane.alignment="CENTER" hgap="10.0" maxHeight="1.7976931348623157E308"
        maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" vgap="10.0">
        <columnConstraints>
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="-Infinity" prefWidth="375.0" />
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="-Infinity" prefWidth="225.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" valignment="BASELINE" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" valignment="BASELINE"
                vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" valignment="BASELINE"
                vgrow="SOMETIMES" />
        </rowConstraints>
        <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </BorderPane.margin>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Label GridPane.columnIndex="1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" text="\${user}">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label GridPane.rowIndex="1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" text="\${auction.name}">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label GridPane.rowIndex="2" text="\${item.name}">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label GridPane.columnIndex="1" GridPane.rowIndex="1" text="\${auction.price}">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label GridPane.columnIndex="1" GridPane.rowIndex="2" text="\${auction.end}">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </GridPane>
    <TabPane VBox.vgrow="ALWAYS" minHeight="-Infinity" minWidth="-Infinity" tabClosingPolicy="UNAVAILABLE"
        tabMaxHeight="100.0" tabMaxWidth="100.0" tabMinWidth="100.0">
        <Tab closable="false" text="Opis">
            <fx:include source="description/AuctionDescriptionView.fxml" fx:id="auctionDescription" />
        </Tab>
        <Tab closable="false" text="Zdjęcia">
            <fx:include source="photos/AuctionPhotosView.fxml" fx:id="auctionPhotos" />
        </Tab>
        <Tab closable="false" text="Historia">
            <fx:include source="history/AuctionHistoryView.fxml" fx:id="auctionHistory" />
        </Tab>
        <Tab closable="false" text="Udział">
            <fx:include source="bid/AuctionBidView.fxml" fx:id="auctionBid" />
        </Tab>
    </TabPane>
</VBox>
