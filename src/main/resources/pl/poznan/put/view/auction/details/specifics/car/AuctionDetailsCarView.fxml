        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label text="Make:" />
    <Label fx:id="makeLabel" GridPane.columnIndex="1" />
    <Label text="Model:" GridPane.rowIndex="1" />
    <Label fx:id="modelLabel" GridPane.rowIndex="1" GridPane.columnIndex="1" />
    <Label text="Mileage:" GridPane.rowIndex="2" />
    <Label fx:id="mileageLabel" GridPane.rowIndex="2" GridPane.columnIndex="1" />
    <Label text="Transmission:" GridPane.rowIndex="3" />
    <Label fx:id="transmissionLabel" GridPane.rowIndex="3" GridPane.columnIndex="1" />
    <Label text="Engine:" GridPane.rowIndex="4" />
    <Label fx:id="engineLabel" GridPane.rowIndex="4" GridPane.columnIndex="1" />
    <Label text="Fuel:" GridPane.rowIndex="5" />
    <Label fx:id="fuelLabel" GridPane.rowIndex="5" GridPane.columnIndex="1" />
    <Label text="Condition:" GridPane.rowIndex="6" />
    <Label fx:id="conditionLabel" GridPane.rowIndex="6" GridPane.columnIndex="1" />
</GridPane>
