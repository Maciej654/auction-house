    <Label text="RAM:" GridPane.rowIndex="4" />
    <Label fx:id="ramLabel" GridPane.rowIndex="4" GridPane.columnIndex="1" />
    <Label text="Operating System:" GridPane.rowIndex="5" />
    <Label fx:id="osLabel" GridPane.rowIndex="5" GridPane.columnIndex="1" />
</GridPane>
