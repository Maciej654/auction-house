        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label text="Producer:" />
    <Label fx:id="producerLabel" GridPane.columnIndex="1" />
    <Label text="Screen size:" GridPane.rowIndex="1" />
    <Label fx:id="screenSizeLabel" GridPane.rowIndex="1" GridPane.columnIndex="1" />
    <Label text="Battery:" GridPane.rowIndex="2" />
    <Label fx:id="batteryLabel" GridPane.rowIndex="2" GridPane.columnIndex="1" />
    <Label text="Processor:" GridPane.rowIndex="3" />
